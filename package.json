{
  "name": "refden-extension",
  "version": "3.1.0",
  "main": "index.js",
  "author": "Mario Perez <mapreal19@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch --progress --mode=development",
    "lint": "eslint app",
    "test": "jest",
    "test:watch": "jest --watch",
    "compress": "webpack --mode=production && zip -r -X app.zip app/",
    "manifest:to-firefox": "cp app/manifest.firefox.json app/manifest.json",
    "manifest:to-chrome": "cp app/manifest.chrome.json app/manifest.json"
  },
  "dependencies": {
    "@sentry/browser": "^5.11.1",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "autosize": "^4.0.2",
    "jquery": "^3.5.1",
    "lodash": "^4.17.13",
    "select2": "^4.0.13",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.10.0",
    "eslint": "^9.1.1",
    "eslint-plugin-jest": "^23.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jsdom": "^16.0.1",
    "regenerator-runtime": "^0.13.5",
    "style-loader": "^3.3.4",
    "web-ext": "^5.4.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-notifier": "^1.15.0"
  },
  "browserslist": "> 0.25%, not dead",
  "jest": {
    "setupFiles": [
      "./app/libs/tests/setup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  }
}
